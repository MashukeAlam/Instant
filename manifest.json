{
    "manifest_version" : 2,
    "name" : "Instant", 
    "version" : "3.3", 
    "background": {
        "scripts" : ["background.js"]
    },
    "permissions" : ["tabs"],
    "browser_action": {
        "default_icon" : "icons8-gamma-48.png",
        "default_popup" : "sketch/popup.html", 
        "default_title" : "Search anything!"
    },
    "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+K",
        "windows": "Alt+S",
        "mac": "Alt+Shift+P"
      }
    }
  }
}